					<?php $setting_theme = get_field('setting_theme', 'option');
            switch ($setting_theme) {
              case 'one':
              get_template_part( 'template-parts/post-meta-s2');
              break;
              case 'two':
              get_template_part( 'template-parts/post-meta-s1');
              break;
              case 'three':
              get_template_part( 'template-parts/post-meta-s1');
                  break;
              case 'four':
              get_template_part( 'template-parts/post-meta-s2');
              break;
              case 'five':
              get_template_part( 'template-parts/post-meta-s2');
              break;
              case 'six':
              get_template_part( 'template-parts/post-meta-s2');
              break;
              case 'seven':
              get_template_part( 'template-parts/post-meta-s2');
              break;
              case 'eight':
              get_template_part( 'template-parts/post-meta-s1');
              break;
              case 'nine':
              get_template_part( 'template-parts/post-meta-s1');
              break;
              case 'ten':
              get_template_part( 'template-parts/post-meta-s1');
              break;
              case 'eleven':
              get_template_part( 'template-parts/post-meta-s1');
              break;
              default:
              get_template_part( 'template-parts/post-meta-s1');
                break;
            }
